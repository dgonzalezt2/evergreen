AnaliticaModel:
    entidades*=Entidad
    operaciones*=Operacion
;

Entidad:
    'entidad' name=ID '{' campos*=Campo '}'
;

Campo:
    name=ID ':' tipo=Tipo
;

Tipo:
    'string' | 'int' | 'float'
;

Operacion:
    ReporteProductores | ReporteProductorPDF
;

ReporteProductores:
    'reporte_consolidado' 'productores' 'nombre' name=ID
    'incluir' campos+=ID[',']
;

ReporteProductorPDF:
    'reporte_pdf' 'productor' 'nombre' name=ID
    'campo' campo=ID
;