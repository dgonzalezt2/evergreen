{
  "version": 2,
  "builds": [
    {
      "src": "front/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://evergreen-backend.up.railway.app/api/:path*"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "front/$1"
    }
  ]
}
